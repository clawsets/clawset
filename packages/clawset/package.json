{
  "name": "clawset",
  "version": "0.1.1",
  "description": "Install and configure OpenClaw presets with a single command",
  "type": "module",
  "bin": {
    "clawset": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx scripts/generate-preset-schema.ts && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist .turbo"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "keywords": [
    "openclaw",
    "cli",
    "preset",
    "automation"
  ],
  "author": "Afshin Mehrabani",
  "license": "MIT",
  "homepage": "https://clawsets.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawsets/clawset"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.0",
    "clawhub": "^0.6.1",
    "execa": "^9.5.0",
    "pacote": "^21.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.21.0",
    "vitest": "^2.1.0",
    "zod-to-json-schema": "^3.25.1"
  }
}
