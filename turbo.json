{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "website#build": {
      "dependsOn": [],
      "outputs": [".next/**", "out/**"],
      "inputs": ["src/**", "../../presets/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "format": {
      "cache": false
    },
    "format:check": {},
    "clean": {
      "cache": false
    }
  }
}
